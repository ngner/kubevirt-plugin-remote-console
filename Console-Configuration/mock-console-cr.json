{
  "apiVersion": "operator.openshift.io/v1",
  "kind": "Console",
  "metadata": {
    "name": "cluster"
  },
  "spec": {
    "customization": {
      "customLogoFile": {
        "name": "custom-logo",
        "key": "logo-file.png"
      },
      "perspectives": [
        {
          "id": "admin",
          "visibility": {
            "state": "Enabled"
          }
        },
        {
          "id": "virtualization-perspective",
          "visibility": {
            "state": "Enabled"
          }
        },
        {
          "id": "dev",
          "visibility": {
            "state": "Disabled"
          }
        }
      ]
    }
  }
}
